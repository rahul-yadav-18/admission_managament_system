
@{
    ViewBag.Title = "Mydetails";
    Layout = "~/Views/Shared/admin.cshtml";
}

<h2 class="text-center txt-mycolor m-3">Mydetails</h2>
<div class="row">
    <div class="col-sm-8 mx-auto bg-light mt-4 pt-3 mb-3">
        
       
                    <form method="post" class="mt-5">
                        <div class="row">
                            <div class="col-sm-6">
                                <b>Email :</b><br />
                                <input type="text" name="email" class="form-control mt-1 mb-3" />
                            </div>
                            <div class="col-sm-6">
                                <b>Mobile No :</b><br />
                                <input type="number" name="mobno" class="form-control mt-1 mb-3" />
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <b>Pincode :</b><br />
                                <input type="number" name="pin" class="form-control mt-1 mb-3" />
                            </div>
                            <div class="col-sm-6">
                                <b>Address :</b><br />
                                <input type="text" name="address" class="form-control mt-1 mb-3" />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <b>State :</b><br />
                                <input type="text" name="state" class="form-control mt-1 mb-3" />
                            </div>
                            <div class="col-sm-6">
                                <b>Country :</b><br />
                                <input type="text" name="country" class="form-control mt-1 mb-3" />
                            </div>

                        </div>
                        <center><button type="submit" class="btn bg-mycolor text-light fw-bold">Submit</button></center>
                    </form>
             
    </div>
</div>

