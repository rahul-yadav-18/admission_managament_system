
@{
    ViewBag.Title = "SubmitedTask";
    Layout = "~/Views/Shared/admin.cshtml";
}

<h2 class="text-center txt-mycolor">Submited Task</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">S.N.</th>
            <th scope="col">Uploaded BY</th>
            <th scope="col">Email</th>
            <th scope="col">Task Name</th>
            <th scope="col">Code File</th>
            <th scope="col">Output File</th>
            <th scope="col">Marking</th>
        </tr>
    </thead>
    <tbody>
        @{
            if (ViewBag.submittask != null)
            {
                if (ViewBag.submittask.Rows.Count > 0)
                {
                    foreach (var st in ViewBag.submittask.Rows)
                    {
                        <tr>
                            <td scope="row">@st[0]</td>
                            <td scope="row">@st[5]</td>
                            <td scope="row">@st[4]</td>
                            <td scope="row">@st[1]</td>
                            <td scope="row"><a href="~/Content/codefile/@st[2]" target="_blank" style="text-decoration:none"><i class="fa fa-download"></i></a></td>
                            <td scope="row"><a href="~/Content/outputfile/@st[3]" target="_blank" style="text-decoration:none"><i class="fa fa-download"></i></a></td>

                            <td scope="row">
                                <form method="post">
                                    <input type="hidden" name="ansid" value="@st[0]" />
                                    Maximum Marks : <input type="number" name="maxnum" placeholder="Enter Maximum number.." /> <br/> Obtain Marks : <input type="number" name="obnum" placeholder="Enter Obtain Marks" />
                                    <input type="submit" value="Assign" />
                                </form>
                            </td>
                        </tr>
                    }
                }
            }
        }
    </tbody>
</table>


